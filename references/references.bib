@article{Burwen2010,
abstract = {Experiments were conducted with a multibeam dual-frequency identification sonar (DIDSON) to evaluate the accuracy and precision of estimating lengths from images of tethered fish insonified at side aspect in an Alaskan river. Live tethered Chinook salmon Oncorhynchus tshawytscha and sockeye salmon O. nerka were suspended in front of a long-range DIDSON (1.2 MHz, 48 beams) equipped with an ultra-high-resolution lens. Lengths measured manually from DIDSON images were highly correlated with the actual lengths (R² = 0.90, RMSE = 5.76 cm). No range dependency in the accuracy of the range estimates was documented. We conclude that relatively accurate and precise estimates of fish length are now possible with certain DIDSON system configurations at up to 21 m.},
annote = {not very useful to us},
author = {Burwen, Debby L. and Fleischman, Steven J. and Miller, James D.},
doi = {10.1577/t09-173.1},
file = {:C\:/Users/ABarros/Downloads/Accuracy and Precision of Salmon Length Estimates Taken from DIDSON Sonar Images.pdf:pdf},
issn = {0002-8487},
journal = {Transactions of the American Fisheries Society},
number = {5},
pages = {1306--1314},
title = {{Accuracy and Precision of Salmon Length Estimates Taken from DIDSON Sonar Images}},
volume = {139},
year = {2010}
}
@article{Denton2016,
abstract = {We used an ARIS 1800 and a DIDSON (LR) multi-beam SONAR unit to monitor Chinook salmon escapement in the Elwha river on a daily basis from June 4 th , 2014 through September 10 th , 2014. We reviewed 20-minutes of each hour long file and then adjusted these raw counts for expansion to full hour counts, species composition, observer error, and filling data gaps. This process yielded a non-jack Chinook escapement estimate above the SONAR sites of 4,360 fish, with a coefficient of variation of 3.5% (95% CI 4,050-4,685). We also provide channel specific estimates of passage for the Hunt Road Channel (HRC), 3,040 (3.1% CV, 95% CI 2,860-3,230) and the Old Mainstem (OM) 1,320 (8.7% CV, 95% CI 1,100-1,545). In addition, we estimated coefficients of variation for each of the sources of uncertainty: observer error (3.6%), 20 minute expansion (3.4%), species composition (2.8%), and data gap fill (>.1%). Fifty percent of the Chinook run had passed the SONAR sites by July 20 th this year, which was 5 days earlier than 2013 and 11 days earlier than 2009-2012. We also caught the first Chinook during species composition netting three weeks earlier this year than in 2013, June 4 th vs. June 25 th. Our escapement estimate of 4,360 non-jack Chinook salmon compares favorably with other data from the watershed. A redd survey covering most of the spawnable watershed on September 17 th , 2014, in combination with collections for the WDFW hatchery yielded an approximate total of 5,200 Chinook salmon. This year's estimate is an improvement on prior years because we placed an additional SONAR in the previously under monitored Old Mainstem channel and we implemented species composition netting. Therefore, we had a more complete data set and were able to estimate CV's for each source of uncertainty in our estimate. Reviewing the large amounts of data produced by the continuous operation of two SONARs in addition to organizing the logistics and field crew needed to maintain the in stream species composition netting will be the biggest challenges facing the project as it moves forward.},
author = {Denton, Keith and Mchenry, Mike and Moses, Raymond and Ward, Eric and Liermann, Martin and Stefankiv, Oleksandr and Wells, Wilson and Pess, George},
file = {:C\:/Users/ABarros/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Denton et al. - 2016 - 2016 Elwha River Chinook Escapement Estimate Based on DIDSON ARIS Multi-Beam SONAR Data.pdf:pdf},
number = {2},
pages = {1--36},
title = {{2016 Elwha River Chinook Escapement Estimate Based on DIDSON / ARIS Multi-Beam SONAR Data}},
volume = {1},
year = {2016}
}
