@article{Able2014,
abstract = {Dual-frequency identification sonar (DIDSON) offers important advantages over other sampling tools for observing pelagic and benthic fishes in situ. Because it relies on sound, DIDSON can detect fish in a non-destructive and non-intrusive manner. In our unique application, the equipment's small size and low power requirements allow deployment from a kayak for increased maneuverability in complex habitats. Characteristics that typify echograms of different fishes can be extracted using multivariate ordination techniques, such as principal components analysis (PCA), with in situ groundtruthing. Here we present reference images, techniques, and human-observer-error estimates from DIDSON application. Together, these approaches enhance our ability to sample fishes and even observe certain behaviors in complex, turbid environments during a full diel cycle. {\textcopyright} AlphaMed Press 2014.},
author = {Able, Kenneth W. and Grothues, Thomas M. and Rackovan, Jenna L. and Buderman, Frances E.},
doi = {10.1656/045.021.0207},
file = {:C\:/Users/ABarros/Downloads/26453585.pdf:pdf},
issn = {10926194},
journal = {Northeastern Naturalist},
number = {2},
pages = {192--209},
title = {{Application of mobile dual-frequency identification sonar (DIDSON) to fish in estuarine habitats}},
volume = {21},
year = {2014}
}
@article{Boswell2008,
abstract = {We present a semiautomated analytical ap- proach incorporating both image and acoustic processing techniques to apply to dual-frequency identification sonar (DIDSON) data. Our objectives were (1) to develop a standardized analysis pathway in order to reduce the effort associated with counting, measuring, and tracking fish targets; and (2) to empirically obtain estimates of basic target information (e.g., size, abundance, speed, and direction of travel). Analyses were conducted on DIDSON data collected at three different locations (the Kenai River, Alaska; Mobile River, Alabama; and Port Fourchon, Louisiana) with different equipment and deployment configurations. We developed an efficient postprocessing approach that can be applied to a variety of data sets, independent of user and deployment method. For two of the three data sets analyzed, the estimates of fish abundance derived from DIDSON analyses were not significantly different from the manual counts of DIDSON files. The analyses produced estimates of mean fish length, direction and speed of travel, and target surface area for all targets within each data set. A consistent analysis platform increases the acceptance and reliability of the DIDSON as a tool for fisheries surveys and further demonstrates the usefulness of DIDSON technology in fisheries applications.},
annote = {Most of this is specific to methods of sonar image processing, and not important to my needs currently.},
author = {Boswell, Kevin M. and Wilson, Matthew P. and Cowan, James H.},
doi = {10.1577/m07-116.1},
file = {:C\:/Users/ABarros/Downloads/N American J Fish Manag - 2011 - Boswell - A Semiautomated Approach to Estimating Fish Size  Abundance  and Behavior from.pdf:pdf},
issn = {0275-5947},
journal = {North American Journal of Fisheries Management},
number = {3},
pages = {799--807},
title = {{A Semiautomated Approach to Estimating Fish Size, Abundance, and Behavior from Dual-Frequency Identification Sonar (DIDSON) Data}},
volume = {28},
year = {2008}
}
